<?xml version="1.0" encoding="utf-8"?>
<LanTalk>
  <Settings>
    <!--传输文件线程数-->
    <node key="ThreadCount" value="1" />
    <!--是否兼容LanMsg-->
    <node key="LanMsg" value="true" />
    <!--发送消息的快捷健，enter表示按回车发送，ctrlenter表示按控制健加回车发送-->
    <node key="SendButton" value="enter" />
    <!--是否为外网用户 true/false-->
    <node key="ISNET" value="false" />
    <!--如果是外网用户，则应配置服务器地址-->
    <node key="ServerAddr" value="127.0.0.1" />
    <!--如果是外网用户，则应配置服务器端口-->
    <node key="ServerPort" value="9050" />
  </Settings>
</LanTalk>