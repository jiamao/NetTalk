<?xml version="1.0" encoding="utf-8" ?>
<configuration>
  <configSections>
  </configSections>
  <connectionStrings>
    <add name="LanTalkServer.Properties.Settings.usersConnectionString"
      connectionString="Provider=Microsoft.Jet.OLEDB.4.0;Data Source=|DataDirectory|\users.mdb;Persist Security Info=True"
      providerName="System.Data.OleDb" />
  </connectionStrings>
  <appSettings>
    <!-- 是否自动启动并开启服务器-->
    <add key="AUTOSTART" value="false"/>
    <!--是否对本地用户监听-->
    <add key="LISTENLOCAL" value="false"/>
    <!--是否为二级服务器-->
    <add key="LEVELSERVER" value="true"/>
    <!--如果为二级服务器此地址才会有效，为主服务器的地址-->
    <add key="SERVER" value="haofefe.gicp.net"/>
    <!--如果为主服务器，此项为主服务器的监听端口，，如果为二级服务器，此端口为远程主服务器的端口，与SERVER项一起使用-->
    <add key="PORT" value="60000"/>
  </appSettings>
</configuration>